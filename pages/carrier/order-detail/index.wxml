<!--pages/carrier/order-detail/index.wxml-->
<view class="container">
	<order-card list="{{order}}" showMore="{{false}}"></order-card>
	<map subkey="{{subKey}}" style="width: 100%; height: 300px;" latitude="{{map.latitude}}" longitude="{{map.longitude}}" scale="{{map.scale}}" markers="{{map.markers}}">
	</map>
	<view class="code-info">
		<text>交货单号：{{code}}</text>
	</view>
	<order-status list="{{list}}"></order-status>
</view>