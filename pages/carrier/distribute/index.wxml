<!--pages/carrier/distribute/index.wxml-->
<view class="container">
	<view class="top-station"></view>
	<view class="top-filter">
		<filter-box id="FilterBox" bind:filterSubmit="filterSubmit">
			<view slot="cont">
				<view class="form-box">
					<form catchsubmit="formSubmit" catchreset="formReset">
						<view class="form-row">
							<view class="form-title">更多搜索条件</view>
						</view>
						<view class="form-row form-column">
							<view class="f-row-label">DN单号</view>
							<view class="f-row-val">
								<input class="form-row-input" data-name="DN_No" name="DN_No" bindinput="bindInput" value="{{parm.DN_No}}" />
								<icon-font class="f-row-icon" type="shanchu4" color="#BBB" bind:iconClick="iconClick" data-name="DN_No"></icon-font>
							</view>
						</view>
						<view class="form-row form-column">
							<view class="f-row-label">到货地址</view>
							<view class="f-row-val">
								<input class="form-row-input" data-name="ArrivalAddress" name="ArrivalAddress" bindinput="bindInput" value="{{parm.ArrivalAddress}}" />
								<icon-font class="f-row-icon" type="shanchu4" color="#BBB" bind:iconClick="iconClick" data-name="ArrivalAddress"></icon-font>
							</view>
						</view>
					</form>
				</view>
			</view>
		</filter-box>
	</view>
	<view class="page-init">
		<view class="my-order">
			<order-card list="{{list}}" bind:navDetail="navDetail"></order-card>
		</view>
		<!-- <fixed-btns addBtn="{{addBtn}}"></fixed-btns> -->
	</view>
</view>