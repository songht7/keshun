<!--pages/components/picker-search/picker-search.wxml-->
<view style="width:{{_width}};height:{{_height}};" class="picker-search">
	<input value="{{picker_value}}" class="picker-item-input" bindinput="bindkeyinput" />
	<picker style="width:30%;height:100%;" bindchange="bindchange" value="{{list2[index][actualvalue]}}" range-key="{{showvalue}}" range="{{list2}}">
		<view class='picker-icon'></view>
	</picker>
</view>