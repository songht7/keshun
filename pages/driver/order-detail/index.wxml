<!--pages/driver/order-detail/index.wxml-->
<view class="container">
	<map wx:if="{{map.latitude&&map.longitude}}" subkey="{{subKey}}" style="width: 100%; height: 300px;" latitude="{{map.latitude}}" longitude="{{map.longitude}}" scale="{{map.scale}}" min-scale="3" max-scale="20" markers="{{map.markers}}">
	</map>
	<view class="code-info">
		<text>交货单号：{{code}}</text>
	</view>
	<order-status list="{{list}}" count="{{count}}"></order-status>
</view>